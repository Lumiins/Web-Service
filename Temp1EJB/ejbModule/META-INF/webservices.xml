<?xml version="1.0" encoding="UTF-8"?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_2.xsd" version="1.2">
	<webservice-description>
		<webservice-description-name>CalculatorService</webservice-description-name>
		<wsdl-file>META-INF/wsdl/calculator.wsdl</wsdl-file>
		<jaxrpc-mapping-file>META-INF/calculator_mapping.xml</jaxrpc-mapping-file>
		<port-component>
			<port-component-name>CalculatorPort</port-component-name>
			<wsdl-port xmlns:pfx="http://superbiz.org/wsdl">
			pfx:CalculatorPort</wsdl-port>
			<enable-mtom>false</enable-mtom>
			<service-endpoint-interface>org.superbiz.CalculatorWs</service-endpoint-interface>
			<service-impl-bean>
				<ejb-link>
				CalculatorServiceSoapBindingImpl</ejb-link>
			</service-impl-bean>
		</port-component>
	</webservice-description>
</webservices>
